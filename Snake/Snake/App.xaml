<Application x:Class="Snake.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Snake"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
		<SolidColorBrush x:Key="OutsideColor">#454545</SolidColorBrush>
		<SolidColorBrush x:Key="GridBackGroundColor">#999999</SolidColorBrush>
		<SolidColorBrush x:Key="GridLineColor">#454545</SolidColorBrush>
		<SolidColorBrush x:Key="TextColor">#EBEBEB</SolidColorBrush>
		<SolidColorBrush x:Key="OverlayColor">#7f000000</SolidColorBrush>

		<FontFamily x:Key="MainFont">Assets/SnakeAssets/#Droid Sans Mono</FontFamily>

	</Application.Resources>
</Application>
